{
    "files.exclude": {
        "out": true,
        "dist": true,
        "node_modules": true,
        ".vscode": true,
        ".vscode-test": true,
        ".gitignore": true,
        ".eslintrc.json": true,
        ".vscodeignore": true,
        "package-lock.json": true,
        "tsconfig.json": true,
        "LICENSE": true,
        "*.md": true,
        ".github": true
    },
    "search.exclude": {
        "out": true,
        "dist": true,
        "node_modules": true
    },
    "typescript.tsc.autoDetect": "off",
    "eslint.run": "onSave",
    "eslint.runtime": "",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.formatOnSave": true,
    "cSpell.words": ["CALLGRAPH", "debricked", "uninstallation"],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
}
